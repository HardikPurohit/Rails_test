<!DOCTYPE html>
<html>
<head>
  <title>Homework</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%unless session[:customer_id].nil? && current_admin.nil?%>
  <% if session[:customer_id].nil?%>
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <ul class="nav navbar-nav">
          <li class="active"><%= link_to "Home", admins_path%></li>
          <li><%=link_to "Cleaners" ,cleaners_path%></li>
          <li><%=link_to "Customer", customers_path %></li>
          <li><%=link_to "City", cities_path%></li>
        </ul>
        <ul class="nav navbar-nav navbar-right">
          <li><%= link_to 'Log out', destroy_admin_session_path,method: :delete, class: "logout" %>
        </ul>
      </div>
    </nav>
  <%else%>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <ul class="nav navbar-nav">
        <li class="active"><%= link_to "Home", customers_path%></li>
        <li><%= link_to "New Booking",new_booking_path%></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><%=link_to "Log out",customer_log_out_path, class: "logout"%></li>
      </ul>
    </div>
  </nav>
  <%end%>
  <%end%>
<div class="container">
  <%= yield %>
</div>
<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
</body>
</html>
