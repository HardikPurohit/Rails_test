<p id="notice"><%= notice %></p>
<table border="2">
  <tr>
    <th colspan="2" align="center">Details of <%=@cleaner.first_name + " " + @cleaner.last_name %></th>
  </tr>
  <tr>
    <td>Email</td>
    <td><%=@cleaner.email%></td>
  </tr>
  <tr>
    <td>Quality Score</td>
    <td><%=@cleaner.quality_score%></td>
  </tr>
  <tr>
    <td>Cities</td>
    <td>
      <%@cleaner_city.each do |city|%>
        <%= city.city_name %><br />
      <%end%>
    </td>
  </tr>
</table>
<%= link_to 'Edit', edit_cleaner_path(@cleaner) %> |
<%= link_to 'Back', cleaners_path %>
